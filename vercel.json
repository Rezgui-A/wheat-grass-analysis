{
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 3009
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9",
    "MAX_CONTENT_LENGTH": "16777216"
  }
}